@model StatiiIncarcare.Models.ViewModels.AddPlugModel;

@{
    ViewData["Title"] = "Adauga priza noua";
}


<h3>Adauga o noua priza</h3>
<form  asp-controller = "Prize" asp-action = "AddPriza" method="post" class ="mt-3">
    <select  class="custom-select custom-select-lg mb-3" asp-for = "IdTip">
        <option selected>Alege tipul prizei</option>
       @foreach(var item in Model.TipPrize)
       {
           <option value ="@item.IdTip">@item.Nume </option>
          
       }
    </select>
    <input readonly asp-for = "@Model.IdStatie" hidden >
   <br />
    <p><button class="btn btn-primary" type="submit">Submit</button></p>
</form>
