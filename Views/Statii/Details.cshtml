         <table class="table table-striped" id = "myTable">
    <thead>
        <tr>
            <th>Numar priza</th>
            <th>Tip Priza</th>
            <th>Rezervare</th>
            <th>Disponibilitate</th>
        </tr>
    </thead>
    <tbody>
   @foreach (var priza in Model.Prizas) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => priza.IdPriza)
            </td>
            <td>
                @Html.DisplayFor(modelItem => priza.IdTipNavigation.Nume)
            </td>
            <td>
              <a   type="button" class="btn btn-success" asp-controller = "Booking" asp-action="NewBooking" asp-route-idPriza="@priza.IdPriza">Rezervare</a>
            </td>
            <td>
               <a  type="button" class="btn btn-outline-dark" asp-controller = "Disponibilitate" asp-action="Index" asp-route-idPriza="@priza.IdPriza">Disponibilitate</a>

            </td>
         </tr>
        
}
    </tbody>
</table>


 